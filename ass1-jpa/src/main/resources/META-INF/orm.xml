<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm    
    http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
    version="2.0">
    <description>ass1-jpa</description>
    <package>entity</package> 
    <entity class="dst.ass1.jpa.model.impl.Computer" name="Computer">        
        <table name="Computer"/>
        <attributes>
            <id name="id">
                <generated-value strategy="AUTO"/>
            </id>
            <basic name="name">
                <column name="name" unique="true"/>
            </basic>
            <basic name="cpus">
                <column name="cpus"/>
            </basic>
            <basic name="location">
                <column name="location"/>
            </basic>
            <basic name="creation">
                <column name="creation"/>
            </basic>
            <basic name="lastUpdate">
                <column name="lastUpdate"/>
            </basic>
            <many-to-one name="cluster" target-entity="dst.ass1.jpa.model.impl.Cluster"/>
            <many-to-many name="executions" target-entity="dst.ass1.jpa.model.impl.Execution"/>
        </attributes>
    </entity>
</entity-mappings>
